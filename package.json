{
  "name": "codefuture",
  "version": "0.0.1",
  "description": "codefuture server",
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production babel src --out-dir dist",
    "serve": "node dist/main",
    "start:babel": "babel-node src/main",
    "start": "cross-env NODE_ENV=develop nodemon --watch src --exec npm run start:babel -L",
    "test": "cross-env NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-core/register test/**/*.js --bail -t 2000000",
    "seeds:develop": "cross-env NODE_ENV=develop babel-node src/seeds/user",
    "seeds:production": "cross-env NODE_ENV=production babel-node src/seeds/user",
    "deploy:dev": "cross-env NODE_ENV=develop pm2 start pm2.config.json",
    "deploy:prod": "cross-env NODE_ENV=production pm2 start pm2.config.json"
  },
  "author": "sunchang",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^3.2.4",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-import": "^1.16.0",
    "install": "^0.8.9",
    "mocha": "^3.5.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-cors": "0.0.16",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa-swagger-decorator": "^1.4.6",
    "lodash": "^4.17.4",
    "loglevel": "^1.6.1",
    "moment": "^2.22.2",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.5",
    "randomstring": "^1.1.5",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sequelize": "^3.33.0",
    "sha1": "^1.1.1",
    "supertest": "^2.0.1"
  },
  "license": "MIT"
}
